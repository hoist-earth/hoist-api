{
  "version": 2,
  "builds": [{ "src": "/dist/index.js", "use": "@now/node-server" }],
  "routes": [
    {
      "src": ".*",
      "dest": "/dist/index.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Headers": "*",
        "Access-Control-Allow-Methods": "*"
      }
    }
  ]
}
